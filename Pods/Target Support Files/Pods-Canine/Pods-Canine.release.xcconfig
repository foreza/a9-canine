FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/AmazonAd/AmazonMobileAds-ios-2.2.15.1-SDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AmazonAd"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AmazonAd"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AdSupport" -framework "AmazonAd" -framework "CoreLocation" -framework "CoreTelephony" -framework "EventKit" -framework "EventKitUI" -framework "JavaScriptCore" -framework "MediaPlayer" -framework "QuartzCore" -framework "SafariServices" -framework "StoreKit" -framework "SystemConfiguration"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
